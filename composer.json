{
    "name": "aoe-elo/backend",
    "type": "project",
    "description": "Backend for aoe-elo.com | AoE Tournament Elo",
    "keywords": [
        "statistics",
        "backend",
        "elo",
        "aoe",
        "aoe2",
        "aoe2de",
        "tournament",
        "api"
    ],
    "license": "AGPL-3.0-or-later",
    "repositories": [
        {
            "type": "path",
            "url": "./dependencies/liquipedia_client_php/"
        }
    ],
    "require": {
        "php": "^8.2",
        "azjezz/psl": "^2.7",
        "guzzlehttp/guzzle": "^7.7",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/socialite": "^5.8",
        "laravel/tinker": "^2.8",
        "liquipedia/api-client": "@dev",
        "nette/utils": "^4.0",
        "socialiteproviders/discord": "^4.2",
        "socialiteproviders/github": "^4.1",
        "socialiteproviders/steam": "^4.2",
        "socialiteproviders/twitch": "^5.3",
        "spatie/laravel-honeypot": "^4.3",
        "spatie/laravel-permission": "^5.10.2",
        "symfony/yaml": "^6.3",
        "vyuldashev/laravel-openapi": "^1.11",
        "webpatser/laravel-countries": "^1.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.23",
        "laravel/breeze": "^1.23",
        "laravel/pint": "^1.11",
        "laravel/sail": "^1.23.4",
        "laravel/telescope": "^4.16.1",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.8.1",
        "pestphp/pest": "^2.18",
        "pestphp/pest-plugin-laravel": "^2.0",
        "reliese/laravel": "^1.2.3",
        "spatie/laravel-ignition": "^2.2",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
